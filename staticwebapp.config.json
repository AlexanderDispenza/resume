{
  "routes": [
    {
      "route": "/*",
      "redirect": "https://www.alexdispenza.com",
      "statusCode": 301
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.css", "/*.js", "/*.png", "/*.jpg", "/*.pdf"]
  },
  "mimeTypes": {
    ".pdf": "application/pdf"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=3600"
  }
}
